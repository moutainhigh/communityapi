package net.okdi.api.entity;


public class BasExpressPrice {
    private Integer id;

    private Long netId;

    private Long takeProvince;

    private Long sendProvince;

    private Integer price;

    private String remark;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Long getNetId() {
        return netId;
    }

    public void setNetId(Long netId) {
        this.netId = netId;
    }

    public Long getTakeProvince() {
        return takeProvince;
    }

    public void setTakeProvince(Long takeProvince) {
        this.takeProvince = takeProvince;
    }

    public Long getSendProvince() {
        return sendProvince;
    }

    public void setSendProvince(Long sendProvince) {
        this.sendProvince = sendProvince;
    }

    public Integer getPrice() {
        return price;
    }

    public void setPrice(Integer price) {
        this.price = price;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}