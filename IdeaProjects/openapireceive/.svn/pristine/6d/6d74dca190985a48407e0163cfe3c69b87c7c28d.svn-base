package net.okdi.apiV4.entity;

import java.util.Date;
/**
 * 动态通知表
 * @author amssy
 *
 */
public class DynamicNotice {

	
	private  Long noticeId;//主键id
	private Long cardId;//帖子id
	private Date createTime;//创建时间
	private Short type;//类型 ：1：点赞   2：关注,3:评论
	private Short isRead;//是否已读 1:未读，2：已读
	private Long fromMemberId;//关注人或者点赞  或者评论人
	private Long toMemberId;//被关注人，或者被点赞的人，或者被回复人
	public Long getNoticeId() {
		return noticeId;
	}
	public void setNoticeId(Long noticeId) {
		this.noticeId = noticeId;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Short getIsRead() {
		return isRead;
	}
	public void setIsRead(Short isRead) {
		this.isRead = isRead;
	}
	public Long getFromMemberId() {
		return fromMemberId;
	}
	public void setFromMemberId(Long fromMemberId) {
		this.fromMemberId = fromMemberId;
	}
	public Long getToMemberId() {
		return toMemberId;
	}
	public void setToMemberId(Long toMemberId) {
		this.toMemberId = toMemberId;
	}
	public Short getType() {
		return type;
	}
	public void setType(Short type) {
		this.type = type;
	}
	public Long getCardId() {
		return cardId;
	}
	public void setCardId(Long cardId) {
		this.cardId = cardId;
	}
	
	
}
